{
  "name": "@adr-express-ts/oauth2",
  "version": "1.0.0",
  "description": "An OAuth2 Server Wrapper for @adr-express-ts/core",
  "main": "lib/index.js",
  "types": "lib",
  "files": [
    "lib/*"
  ],
  "scripts": {
    "generate:docs": "rm -rf ./docs/ && yarn build && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose && node ./scripts/build-docs.js",
    "build": "yarn lint && rm -rf ./lib/ && tsc -p .",
    "lint": "eslint src/**/*.ts",
    "read:docs": "serve ./docs",
    "version": "yarn build"
  },
  "keywords": [
    "OAuth2",
    "Security",
    "OAuth2 Server",
    "OAuth",
    "Express",
    "express",
    "typescript",
    "TypeScript",
    "Decorator",
    "Decorators",
    "decorator",
    "decorators",
    "controller",
    "Controller",
    "action",
    "domain",
    "responder",
    "Action",
    "Domain",
    "Responder",
    "Inject",
    "inject",
    "ES2017",
    "ES6",
    "ES5"
  ],
  "repository": "https://github.com/RaresAil/aet-oauth2.git",
  "author": "Nicolae-Rares Ailincai",
  "bugs": {
    "url": "https://github.com/RaresAil/aet-oauth2/issues"
  },
  "homepage": "https://github.com/RaresAil/aet-oauth2#readme",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/oauth2-server": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "braintree-jsdoc-template": "^3.3.0",
    "eslint": "^7.26.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-security": "^1.4.0",
    "express": "^4.17.1",
    "fs-extra": "^10.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-escape-at": "^1.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "oauth2-server": "^3.1.1"
  }
}
